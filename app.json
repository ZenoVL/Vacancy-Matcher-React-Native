{
  "expo": {
    "name": "vacancy-matcher-app-react-native",
    "slug": "vacancy-matcher-app-react-native",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "owner": "thethreemusketeers",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#200752"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.1"
    },
    "android": {
      "versionCode": 2,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBdc8kD_R252jRpwhBF4zzk5FIqICqrSvU"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.zenovanleemput.vacancymatcherappreactnative",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Deze app heeft toegang tot je foto's nodig om foto's te kunnen uploaden.",
          "cameraPermission": "Deze app heeft toegang tot je camera nodig om foto's te kunnen uploaden."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Laat $(PRODUCT_NAME) je locatie gebruiken."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7057d9f0-4ef0-405b-a978-ce6d2b63f4fc"
      }
    }
  }
}
